<% include ../header.ejs %>
    <table class="table table-bordered table-hover">
        <tr>
            <td>제목</td>
            <td>작성일</td>
            <td>삭제</td>
        </tr>

        <% contacts.forEach( function(contact) { %>
        	<tr>
	            <td>
	            	<a href="/contacts/detail/<%=contact.id%>">
	            		<%=contact.title%>	
	            	</a>
	            </td>
	            <td>
	                <%=contact.getDate.year%>년
	                <%=contact.getDate.month%>월
	                <%=contact.getDate.day%>일
	            </td>
	            
                <td><a href="/contacts/delete/<%=contact.id%>" class="btn btn-danger" onclick="return confirm('삭제하시겠습니까?')">삭제</a></td>
	            
	        </tr>

        <% }); %>
        
        
    </table>

    <a href="/contacts/write" class="btn btn-default">작성하기</a>

<% include ../footer.ejs %>